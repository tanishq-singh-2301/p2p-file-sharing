{
    "git.enabled": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.crswap": true,
        ".vscode": true,
        "node_modules": true
    },
    "editor.insertSpaces": true,
    "vim.disableExtension": true,
    "editor.tabSize": 4,
    "editor.useTabStops": true
}